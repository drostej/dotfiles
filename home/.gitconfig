[user]
	name = drostej
	email = juergen.droste@telefonica.com
[branch]
	autosetuprebase = always
[push]
	default = matching
[core]
	excludesfile = /Users/drostej/.gitignore_global
	autocrlf = false
[difftool "sourcetree"]
	cmd = /usr/local/Cellar/kdiff3/0.9.98/kdiff3.app/Contents/MacOS/kdiff3 \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /usr/local/Cellar/kdiff3/0.9.98/kdiff3.app/Contents/MacOS/kdiff3 \"$BASE\" \"$LOCAL\" \"$REMOTE\" -o \"$MERGED\"
	trustExitCode = true
